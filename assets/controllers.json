{
    "controllers": {
        "@habib/datatables": {
            "datatable": {
                "enabled": true,
                "fetch": "eager",
                "autoimport": {
                    "datatables.net-dt/css/dataTables.dataTables.min.css": true
                }
            }
        },
        "@symfony/ux-turbo": {
            "turbo-core": {
                "enabled": true,
                "fetch": "eager"
            },
            "mercure-turbo-stream": {
                "enabled": false,
                "fetch": "eager"
            }
        }
    },
    "entrypoints": []
}
